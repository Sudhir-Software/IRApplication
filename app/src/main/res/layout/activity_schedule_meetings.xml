<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.irinspection.irapplication.Schedule_meetings">

    <Button
        android:id="@+id/request_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/holo_green_light"
        android:text="Request"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="437dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="78dp"
        android:layout_height="22dp"
        android:text="Comments"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="288dp" />

    <TextView
        android:id="@+id/from_input"
        android:layout_width="213dp"
        android:layout_height="40dp"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="120dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="71dp"
        android:layout_height="38dp"
        android:text="From : "
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="120dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="71dp"
        android:layout_height="38dp"
        android:text="Location : "
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="169dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="71dp"
        android:layout_height="38dp"
        android:text="Preffer Timings"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="223dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="71dp"
        android:layout_height="38dp"
        android:text="Operation : "
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="65dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="226dp"
        android:layout_height="23dp"
        android:text="Request for an appoinment"
        android:textAppearance="@style/TextAppearance.AppCompat.Button"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/operationType_input"
        android:layout_width="216dp"
        android:layout_height="44dp"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="65dp" />

    <EditText
        android:id="@+id/date_input"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="date"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="223dp" />

    <EditText
        android:id="@+id/postalAddress_input"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPostalAddress"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="169dp" />

    <EditText
        android:id="@+id/mutipleText_input"
        android:layout_width="343dp"
        android:layout_height="107dp"
        android:ems="10"
        android:inputType="textMultiLine"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="319dp" />
</android.support.constraint.ConstraintLayout>
